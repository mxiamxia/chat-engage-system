        "body-parser": "~1.13.2",
        "cheerio": "^0.20.0",
        "coffee-script": "^1.10.0",
        "cookie-parser": "~1.3.5",
        "cors": "^2.7.1",
        "debug": "~2.2.0",
        "ejs": "*",
        "eventproxy": "^0.3.4",
        "express": "~4.13.1",
        "hubot": "^2.19.0",
        "hubot-matteruser": "file:plugins/hubot-matteruser-0.1.3.tgz",
        "ioredis": "^1.15.1",
        "lodash": "^4.11.0",
        "log4js": "^0.6.33",
        "mochawesome": "^1.3.5",
        "moment": "^2.12.0",
        "mongoose": "^4.5.1",
        "morgan": "~1.6.1",
        "pg": "^6.0.3",
        "q": "^1.4.1",
        "redis": "^2.6.2",
        "request": "^2.72.0",
        "serve-favicon": "~2.3.0",
        "socket.io": "^1.4.5",
        "socket.io-client": "^1.4.5",
        "socketio-jwt": "^4.4.0",
